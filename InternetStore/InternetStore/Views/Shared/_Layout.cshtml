    @* <link rel="stylesheet" href="~/InternetStore.styles.css" asp-append-version="true" /> *@
    <link rel="stylesheet" href="~/css/main.css">
</head>
<body>
    <div class="wrapper">
        <div class="header">
            <div class="container">
                <div class="header-top d-flex justify-content-between">
                    <ul class="languages list-inline m-0">
